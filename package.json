{
  "description": "Lib starter kit",
  "author": "Igor Suvorov <hi@isuvorov.com>",
  "license": "MIT",
  "repository": "https://github.com/isuvorov/lib-starter-kit",
  "keywords": [],
  "config": {
    "cz-customizable": {
      "config": ".czrc.js"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "scripts": {
    "commit": "./node_modules/cz-customizable/standalone.js -S",
    "release2": "release-it",
    "cz": "git-cz",
    "cz-release": "standard-version",
    "dev": "BUILD_PARAMS=--copy-files ./scripts/dev.sh",
    "dev:lib1": "npm run watch --scope @lib-initial-kit/lib1",
    "release": "BUILD_PARAMS=--copy-files lerna publish --skip-npm",
    "build-locales": "./scripts/build-locales.sh",
    "bootstrap": "./scripts/bootstrap.sh",
    "update": "lerna exec -- npm run update",
    "update-force": "lerna exec -- ../../scripts/package-update.sh",
    "build2": "lerna exec -- npm run build",
    "build": "BUILD_PARAMS=--copy-files ./scripts/build.sh",
    "watch": "BUILD_PARAMS=--copy-files lerna run --parallel watch",
    "link": "lerna exec -- npm run link",
    "bump": "lerna exec -- npm run bump",
    "storybook": "./scripts/storybook.sh",
    "format-check": "prettier --list-different \"./**/*.js\"",
    "pre-commit": "echo \"IGNORE npm run lint-staged\"",
    "pre-push": "echo \"npm run format-check\"",
    "prettier": "prettier",
    "lint": "npm run lint:js && npm run lint:md .",
    "lint-fix": "eslint . --ext .js --fix && prettier --write --plugin-search-dir=.",
    "lint:ci": "npm runlint:js --format ./node_modules/eslint-teamcity/index.js . && npm lint:ts **/*.ts && npm lint:md .",
    "lint:js": "lerna exec -- npm run lint:js",
    "lint:js2": "cross-env NODE_ENV=production eslint --cache --cache-location=.cache/eslint --ext .js,.jsx,.json --report-unused-disable-directives packages/*/src",
    "lint:md": "remark -q .",
    "lint:ts": "tslint -p . -c tslint.json -t stylish",
    "spellcheck": "yaspeller *.md",
    "size": "size-limit",
    "size:why": "size-limit --why",
    "jest": "jest --coverage --verbose --color",
    "test": "npm run lint && npm run spellcheck",
    "pretty-staged": "pretty-quick --staged",
    "lint-staged": "lint-staged",
    "publish:latest": "lerna publish --exact --concurrency 1 --force-publish",
    "publish:next": "npm run publish:latest -- --npm-tag=next",
    "publish:debug": "npm run publish:latest -- --npm-tag=debug --no-push"
  },
  "files": [
    "build/"
  ],
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0-beta.42",
    "@babel/preset-stage-0": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.4.3",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@commitlint/prompt": "^8.3.5",
    "@lskjs/linkall": "^0.33.5",
    "@lskjs/log": "^0.35.66",
    "@release-it/conventional-changelog": "1.1.0",
    "@storybook/react": "^5.0.6",
    "@types/react": "^16.8.13",
    "@types/storybook__react": "^4.0.1",
    "babel-eslint": "8.2.3",
    "babel-jest": "^24.7.1",
    "babel-plugin-emotion": "9.2.11",
    "babel-plugin-root-import": "^6.1.0",
    "babel-plugin-transform-class-property-arrow-to-bind": "^1.1.0",
    "babel-preset-minify": "^0.5.0",
    "commitizen": "^4.0.3",
    "company-commit": "^1.1.1",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^3.1.0",
    "cz-customizable": "6.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "glob": "^7.1.3",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "lerna": "^3.13.1",
    "lint-staged": "^8.2.1",
    "nodemon": "^1.19.3",
    "npm-check-updates": "^3.1.8",
    "prettier": "1.19.1",
    "pretty-quick": "^1.10.0",
    "react": "^16.8.6",
    "react-styleguidist": "^9.0.6",
    "react-test-renderer": "^16.8.6",
    "release-it": "12.4.3",
    "remark": "^10.0.1",
    "remark-cli": "^6.0.1",
    "remark-preset-lint-recommended": "^3.0.2",
    "size-limit": "^1.0.0",
    "standard-version": "^7.1.0",
    "storybox": "^1.1.0",
    "typescript": "^3.4.2",
    "webpack": "^4.29.6",
    "yaspeller": "^5.0.1"
  },
  "dependencies": {
    "@lskjs/build-locales": "^0.35.57",
    "@lskjs/dev": "^0.33.10",
    "emotion": "^10.0.9",
    "emotion-server": "^10.0.9",
    "react-router-dom": "5.1.2",
    "telegraf": "^3.30.1"
  }
}
